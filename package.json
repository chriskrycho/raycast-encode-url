{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "encode-url",
  "title": "Encode URL",
  "description": "Encodes a URL for use in e.g. email.",
  "icon": "command-icon.png",
  "author": "chriskrycho",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Encode URL",
      "description": "Encode a URL for use in e.g. mailto links",
      "mode": "no-view",
      "arguments": [
        {
          "name": "text",
          "required": true,
          "type": "text",
          "placeholder": "text to encode"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "volta": {
    "node": "18.17.0",
    "pnpm": "8.6.11"
  }
}
